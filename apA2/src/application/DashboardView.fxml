<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<GridPane fx:controller="application.DashboardController" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10">
    <Label text="Welcome to Your Dashboard" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label fx:id="welcomeLabel" text="" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button text="Edit Profile" onAction="#editProfile" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <Button text="Add Post" onAction="#addPost" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Button text="Retrieve Posts" onAction="#viewPosts" GridPane.columnIndex="0" GridPane.rowIndex="4" />
    
    <Button text="Logout" onAction="#logout" GridPane.columnIndex="0" GridPane.rowIndex="5" />
</GridPane>
